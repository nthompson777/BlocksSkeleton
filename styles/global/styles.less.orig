// Table Styles
// -----------------------------------
// Firefox fieldset bug for styling that involves width of responsive tables
.table-responsive {
  border: none;

  @-moz-document url-prefix() {

    fieldset {
      display: table-cell;
    }
  }
}

// Table Mixin - MGP Default Table Style - Expanded size table
.table-expanded,
.table-default {
  .table();
  .table-bordered();
  .table-striped();
  .table-hover();
}

// Table Mixin - Compacted/Condensed Table - For tables with Large Amounts of data
.table-condensed {
  .table();
  .table-condensed();
  .table-bordered();
  .table-striped();
  .table-hover();
}

.table-expanded,
.table-default,
.table-condensed {

  th {
    font-weight: bold;
    border: 1px solid @table-border-color;
    color: @table-heading-text-color;
    background: @table-heading-bg-color;
  }

  > thead > tr > th {
    border-bottom: none;
  }

  > tbody > tr > th {
    color: @text-color;
  }
}

// Fixed Floating Table Header
.floatThead-container {
  background: @table-fixed-heading-bg-color;  
}

table {

  h3, .h3,
  h4, .h4,
  h5, .h5 {
    margin: 2px 0 !important;
  }

  h4 {
    font-size: 1.2em;
  }

  h5 {
    font-weight: bold;
  }

  .joined-header {
    border-bottom: hidden !important;
    text-align: center;
  }

  .delete-row-cue {
    background-color: @gray-lighter;
    opacity: 0.5;
  }
  // Set min-width of form fields within tables so fields can accomodate a minimum of 7 digits (millions) and so form fields don't collapse on smaller viewports
  .form-control {
    min-width: 120px; //120px smallest value without cropping mm/dd/yyyy inputs
  }

  tr {

    &.primary,
    &.secondary,
    &.tertiary {

      > th {
        color: #fff;
      }
    }
  }
}

// Mixins - Hightlight entire columns with "colgroups"
// (example - see SS Maximization table)
.colgroup-highlight(primary; @brand-primary);
.colgroup-highlight(selected; @brand-primary);
.colgroup-highlight(secondary; @brand-secondary);
.colgroup-highlight(success; @state-success-bg);
.colgroup-highlight(error; @state-danger-bg);
.colgroup-highlight(warning; @state-warning-bg);
.colgroup-highlight(info; @state-info-bg);

// Mixins - Hightlight entire cell with border
// (example - see Risk/Reward table)
.td-highlight(outline-primary; @brand-primary);
.td-highlight(outline-secondary; @brand-secondary);


// Auto Counter Increments for Tables ( add <div class="numbered-col"></div> to the headers - i.e.- see new Soc Sec Max)
table {
  counter-reset: section;

  .numbered-col {
    counter-reset: subsection;
    background: @state-info-bg;
    text-align: center;
    font-size: 0.8em;
    color: @brand-primary;
    margin: 0 0 15px; // -8px for flush at top
  }

  .numbered-col:before {
    counter-increment: section;
    content: counter(section);
  }

  &.ssMax > thead > tr > th {
    vertical-align: top;
  }
}

tbody > tr:empty {
  display: none;
}

// Table Row Variant Styles - uses "table-row-variant" mixin
// use table row class "totals" for highlighted rows w/ totals
.table-row-variant(totals; @gray-lighter); /*Insts were not liking this green*/
.table-row-variant(primary; @brand-primary);
.table-row-variant(secondary; @brand-secondary);
.table-row-variant(tertiary; @brand-tertiary);

.table > tbody > tr.totals > td,
.table > tbody > tr.info > td {
  font-weight: bold;
}

.highlight-col {
  background-color: @state-info-bg; /*@brand-primary-light;*/
}

.hover-highlight {
  padding: 5px 0;

  &:hover {
    background-color: @table-bg-hover;
  }
}



//Make indents accessible to all elements
.table > tbody > tr > td.indent-1,
.indent-1 {
  padding-left: 1em;
}

.table > tbody > tr > td.indent-2,
.indent-2 {
  padding-left: 2em;
}

.table > tbody > tr > td.indent-3,
.indent-3 {
  padding-left: 3em;
}

//Tab indents for complex tables
//with nested rows
.table {
  tr.spacer {
    background: none !important;
    border: none;
  }
}


// Off-canvas Menus for Options, Calculators, & Other Features: Base Style for ALL VIEWPORT SIZES!!
//-------------------------------------
.offcanvas-menu {
  position: relative;
  -webkit-transition: all .25s ease-in-out;
  -o-transition: all .25s ease-in-out;
  -moz-transition: all .25s ease-in-out;
  transition: all .25s ease-in-out;
}

.offcanvas-left {
  left: 0;

  .offcanvas-default-menu {
    display: none;
    left: -30%;
  }
}

.offcanvas-left.active-menu-item {
  left: 30%;
  margin-left: -15px;
}

.offcanvas-default-menu {
  position: absolute;
  top: 0;
  width: 27%;

  .fa {
    margin-right: 5px;
  }
}

.active-menu-item {

  .offcanvas-default-menu {
    display: block;
  }

  button {
    float: right;
  }
}

