    }

    > ul {
      list-style: none;
    }
    /* &:hover {
		background-color: @gray-lighter;
	} */
    &:hover {
      background-color: transparent;
    }
  }
  /*a.super-solve-button:hover {
    background-color:@btn-secondary-bg;
  }*/
}

a.dropdown-toggle {
  cursor: pointer;

  &:hover,
  &:focus {
