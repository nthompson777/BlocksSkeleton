// Dropdown Accordion Menu
.dropdown-accordion.full-width .dropdown-menu {
  width: 100%;

  .panel, .panel-heading, .panel-group {
    max-width: 100%;
  }
}

.accordion-menu.accordion-menu-xl {
  #resources-menu .panel-group {
    max-width: 400px;
  }
}

.dropdown-accordion.accordion-menu .dropdown-menu {
  padding: 0;

  .panel, .panel-heading, .panel-group {
    display: block !important;
    padding: 0 !important;
    margin: 0 !important;
    border: none !important;
  }

  .panel-heading > [aria-expanded="true"] {
    border-bottom: 1px solid #FFF;
  }

  .panel-heading a, .panel-body a, .panel > a {
    display: block;
    position: relative;
    padding: 10px;
    font-size: 14px;
    line-height: 16px;
    font-weight: normal;
    outline: none;
    border-top: 1px solid #DDD;
    border-bottom: 1px solid #DDD;
    margin-top: -1px;
    background: #F9F9F9;
    .transition(~"background .25s ease-in-out, color .25s ease-in-out");

    &:hover {
      text-decoration: none;
      background: #EFEFEF;
    }

    &:focus {
      text-decoration: none;
    }

    span, &:after {
      display: inline-block;
      vertical-align: middle;
    }

    &:after {
      width: 20px;
      text-align: center;
    }

    span {
      width: calc(~"100% - 28px");
    }
  }

  .panel-body {
    padding: 0 !important;

    ul {
      margin: 0;
      padding: 0;
    }

    .cat-name {
      display: block;
      width: 100%;
      padding: 5px 10px;
      font-size: .95em;
      background: @gray-lighter;
      color: @text-color;
    }

    a {
      background: #FFF;
      font-size: .95em;
      padding-left: 20px;

      &:hover {
        background: @gray-lightest;
      }

      &.active-sub {
        background: lighten(@brand-secondary, 15%);
        color: #FFF;
      }
    }

    & > a {
      &:first-child {
        border-top: none;
        .box-shadow(inset 0 10px 7px -11px rgba(0,0,0,.35));
      }

      &:last-child {
        position: relative;
        z-index: 5;
        border-bottom-color: #FFF;
        .box-shadow(inset 0 -10px 7px -11px rgba(0,0,0,.35));
      }

      &:first-child:last-child {
        .box-shadow(~"inset 0 10px 7px -11px rgba(0,0,0,.35),inset 0 -10px 7px -11px rgba(0,0,0,.35)");
      }
    }

    & > p {
      padding: 0;
    }

    & > ul:first-child {
      & > li .cat-name {
        .box-shadow(inset 0 10px 7px -11px rgba(0,0,0,.35));
      }
    }

    & > ul:last-child {
      & > li > ul li:last-child a {
        position: relative;
        z-index: 5;
        border-bottom-color: #FFF;
        margin-bottom: -1px;
        .box-shadow(inset 0 -10px 7px -11px rgba(0,0,0,.35));
      }
    }
  }

  .panel:last-child .panel-body {
    & > a:last-child {
      .box-shadow(~"none !important");
    }
  }
  //Needed for double level menus, i.e. Insurance Policies
  ul > li > ul a:after {
    content: "";
  }
}


// Form Progress Bar (App progression - De-balled/Neutered Style)
//------------------------------------
ul.progressbar {
  margin: 0 auto 18px -35px; //knock 10px off per Tony 5.19.2016
  display: block;
  min-height: 50px;
  z-index: 2;
  overflow: visible;

  .social-security-navigation & {
    margin-bottom: 30px;
  }

  a:hover {
    text-decoration: none;
  }

  .active {

    a {
      color: @brand-secondary;

      &:hover {
        text-decoration: none;
      }
    }
  }

  .dropdown-toggle:after {
    font-family: FontAwesome;
    content: '\f0d7';
    margin-left: 5px;
  }

  .dropdown-menu {
    margin-top: 20px;
    left: 40%;

    > li {

      a.active-page {
        background: @brand-secondary!important;
        color: @font-color-for-darkBG;
      }

      &.active-sub,
      > a.active-sub {
        background: @brand-secondary;
        color: @font-color-for-darkBG;
      }

      > a {
        padding: 5px;
        color: @brand-primary;
      }
    }
  }

  .caret {
    position: relative;
    top: 46px;
    z-index: 110;
  }

  > li {
    list-style-type: none;
    color: #ccc;
    font-size: 1em;
    float: left;
    position: relative;
    text-align: center;
    width: 25%;

    > a,
    a.dropdown-toggle {
      position: relative;
      top: 22px;
      // padding-top: 40px;
      z-index: 110; // Keep this z-index higher so the circle step is clickable and "part of" the anchor
      font-size: 1.2em;
    }
    // Progressbar connectors
    &:before {
      content: '';
      width: 53%;
      position: absolute;
      left: -25%;
      border: solid 2px @gray-lighter;
      top: 33px;
      z-index: 1;
    }
    // Connector not needed before the first step:
    &:first-child:before {
      content: none;
      margin-left: -30px;
    }

    &.active:after {
      background: @brand-secondary;
      color: #fff;
      border: 0;
    }
    .circle-text {
      font-family: @font-family-sans-serif;
      font-size: .9em;
      margin:-1px 0 0 1px;
    }
  }
}


