  }
}

// Partner Mapping/Integration Styles
// ----------------------------------------
#partner-mapping .table { 
  table-layout: fixed; 

  .form-control { min-width: initial; }

  tr {
    td { vertical-align: middle; }
  }

  thead {
    tr th {
      width: 70px;
      &.md { width: 120px; }
      &.lg { width: 220px; }
    }
  }

  & > tbody > tr {
    &:hover {background: initial; }
    &:nth-of-type(odd):hover { background: #f9f9f9; }
  }
}


// Breakpoint for Left Offcanvas Menu
// ----------------------------------------
@media (max-width: 600px) {
  .active-menu-item button {
    float: none;
    display: block;
    margin-top: 15px;
  }

  .offcanvas-left {
    .offcanvas-default-menu {
      left: -95%;
    }

    &.active-menu-item {
      left: 100%;
    }
  }

  .offcanvas-default-menu {
    position: absolute;
    top: 0;
    min-width: 80%;
  }
}
