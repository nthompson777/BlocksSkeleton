/// <reference path="global.less" />

//  Global & Scaffolding Styles
// -----------------------------------
html {
  position: relative;
  min-height: 100%;
}

body {
  background-color: @bg-light; 
  font-size: 14px;
  margin:0;
}

body > .bootstrap-wrapper + .container {
  padding-top: 20px;
}

#maincontent {
 padding-bottom: 50px;  //pad content so footer is spaced correctly
}

select optgroup {
  font-weight: bold;

  & > option {
    font-weight: normal;
  }
}

section {
  padding: 0 0 30px;

  > .row {
    h1, h2 {
      &:first-child {
        margin-top: 0;
      }
    }
  }

  section:last-child {
    padding: 0;
  }

  + hr {
    margin-top: 10px;
  }
}

.container {

  > h1 {
    margin-top: 0;
  }
}

a {
  -webkit-transition: color 0.7s ease-in-out;
  -moz-transition: color 0.7s ease-in-out;
  -o-transition: color 0.7s ease-in-out;
  transition: color 0.7s ease-in-out;

  &:focus {
    outline: none;
  }
}

.highlighted-result {
  padding: 3px 7px;
  margin: 0 5px;
  background: @state-info-bg; /*@brand-primary-light;*/
  display: inline-block;
  border-radius: 3px;
  font-weight: bold;
  color: @gray-light;

  &:empty {
    display: none;
  }
}

.description { // Styled descriptive text with readability presentation
  line-height: 25px;
  margin-top: 10px;
}

// "Last Updated" Date display style
.last-updated {
  margin: 5px 0 20px;
  float: right;

  .date {
    font-weight: bold;
    margin-left: 5px;
    // color: @brand-primary;
    &:after {
      font-family: 'FontAwesome';
      content: '\f274';
      margin-left: 8px;
    }
  }
}

.center-block {
  float: none;
}


// Centered Columns Styles --  NOTE: use sparingly in rare instances since our application is not a centered block style app!!!
.row-centered {
  text-align: center;
}

.col-centered {
  display: inline-block;
  float: none;
  /* reset the text-align */
  text-align: left;
  /* inline-block space fix */
  margin-right: -4px;
}


// Bootstrap has invisible class (visibility: hidden;), but no visible class (visibility: visible;)
.visible {
  visibility: visible;
}


//Improvements for ViewOnly Mode - particularly for selects and input radio
input[readonly],
input[disabled],
input.disabled,
.form-control[readonly],
.form-control[disabled],
.form-control.disabled {
  pointer-events: none;
  background-color: #eee;
}


// Sec 508 Accessibility Styles
// -----------------------------------------------

// Custom default :focus styles that work with "What-Input"
// Mouse Navigation Style
/* [data-whatinput='mouse'] :focus {
  border: none;
} */

//Hide all desc tags used in HighCharts from Screen Readers
desc {
  display: none;
}

// Keyboard Navigation Style
[data-whatinput='keyboard'] :focus {
  .accessibility-focus();  // Accessibility Mixin
  border: dotted 1px lighten(@brand-primary, 7%);
}

// Touch Navigation Style
[data-whatinput='touch'] :focus {
  .accessibility-focus();  // Accessibility Mixin
}

//Added for fieldsets/legends b/c Yes No Toggles and radios and risk tol questions
legend {
  color: @text-color;
  font-size: 1em; 
  border-bottom: none;
  margin-bottom: 0;

  &.control-label { 
    font-weight: bold;
  }
  &.question-label { 
    display: inline;
  }
}

//Default color doesn't pass 508 color contrast test
.help-block {
  color: @text-color;
}

/*Recommended in Bootstrap 3 Docs - http://getbootstrap.com/getting-started/#accessibility*/
#maincontent:focus, 
#zoomer-body:focus { 
  outline: none!important; 
}

// Global CTA (Call to Action) Icons
// -----------------------------------
.add-item:after {
  .fa-icon-standard('\f055'; @link-color);
  .fa-lg();
}

.edit-item:after {
  .fa-icon-standard('\f044'; @link-color);
  .fa-lg();
}

.delete-item:after {
  .fa-icon-standard('\f00d'; @brand-danger);
  .fa-lg();
}

.save-item:after {
  .fa-icon-standard('\f0c7'; @link-color);
  .fa-lg();
}

.copy-plan:after {
  .fa-icon-standard('\f0c5'; @link-color);
  .fa-lg();
}

.fixed-width:after {
  width: 0.92em;
  text-align: center;
}

// Global Stars
// -----------------------------------
.starfull:before {
  .fa-icon-standard('\f005'; @star-color; 0 .1em 0 .1em; default);
  .fa-lg();
}
.starhalf:before {
  .fa-icon-standard('\f123'; @star-color; 0 .1em 0 .1em; default);
  .fa-lg();
}
.starempty:before {
  .fa-icon-standard('\f006'; @star-color; 0 .1em 0 .1em; default);
  .fa-lg();
}


// Type
// ------------------------------------
h1, .h1,
h2, .h2,
h3, .h3,
h4, .h4,
h5, .h5,
h6, .h6 {
  margin-top: ceil((@line-height-computed / 2)); // orig 1.34
  margin-bottom: floor((@line-height-computed / 1));
}

h1, .h1 {
  color: inherit; // keep the h1's the same font-color as our body text
}

h2, .h2,
h3, .h3 {
  color: @brand-primary; // Differentiate the h2 & h3 font-colors for sectioning and ease of readability
}

h4, .h4,
h5, .h5,
h6, .h6 {
  color: inherit; // keep these headers the same font-color as our body text
}

h5 {
  font-weight: bold;
}

@font-size-h1: floor((@font-size-base * 2.4)); // ~33px
@font-size-h2: floor((@font-size-base * 2.0)); // ~28px
@font-size-h3: ceil((@font-size-base * 1.6)); // ~23px
@font-size-h4: ceil((@font-size-base * 1.25)); // ~18px
@font-size-h5: @font-size-base; // ~14px
@font-size-h6: ceil((@font-size-base * 0.85)); // ~12px

p {
  margin: 0 0 ceil((@line-height-computed / 1.34));
}


//Use for cases with the table or panel heading color isn't web accessible b/c of low contrast 
//with links or explain icons in the heading (i.e. link/icon and heading background color both dark)
th .help-modal:after,
.panel-heading .help-modal:after,
.header-link {
  color: @link-color-heading-action;
}

.header-link,
.header-link:hover {
  text-decoration: underline;
}	


// Footer Styles
// -----------------------------------
footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  clear: both;
  background: @footer-bg;
  color: @footer-text-color;
  font-size: 0.95em;
  text-align: center;
  //padding: 10px 0;  // DO NOT PUT PADDING/MARGIN ON FOOTER since it will overlap body content, instead put padding/margin on the ELEMENTS WITHIN the footer 

  p.copyright-text,
  p.broker-disclosure-text,
  p.advisor-disclosure-text {
    /*display: table-cell;*/
    max-width: 1140px;
    vertical-align: middle;
    color: @footer-text-color;
    font-size: 0.95em;
    margin: 8px auto;
  }

  p.copyright-text {
    padding: 20px 0;
  }

  p.broker-disclosure-text,
  p.advisor-disclosure-text {
    padding: 5px 0;

    + p.copyright-text {
      padding: 5px 0;
    }
  }

  a {
    color: @footer-text-color;
    text-decoration: underline;

    &:hover {
      color: @footer-text-color;
    }
  }
}


// Nav Styles
// -----------------------------------
.navbar {
  min-height: 60px;
  height: auto;
}

.navbar-default {
  z-index: 1001; /*set this to prevent ui dialog for being overlayed*/
  background: #fff;
  border-bottom: solid 1px #efefef;
  box-shadow: 0 25px 20px -29px rgba(0,0,0,0.35);

  .navbar-nav .open .dropdown-menu > li > a {
    color: @link-color;

    &:focus, &:hover {
      color: @link-hover-color;
      text-decoration: underline;
    }
  }

  /*&.navbar-fixed-top {   // Removed this 08.03.16 since we don't have a fixed nav anymore (only on XS viewports)
    position: absolute;
  }*/
}

.navbar-collapse.collapse {
  padding-bottom: 5px;
}

.navbar-nav {
  margin-top: 0;
  float: right;
  /*&.navbar-right {
    margin-right: 0;
  }*/
}

#navbar .nav > li:last-child > a {
  padding-right: 0;
}


// Presentation Mode
//------------------------------------
[data-present-fixed-nav="true"] { 
  position: fixed; 
  background-color: fade(#FFFFFF, 90%);
  z-index: 1002; 
  top: 0;
  border-bottom: 1px solid #efefef;
  box-shadow: 0 25px 20px -29px rgba(0,0,0,.35);
  padding-top: 20px;
}

[data-present-fixed-nav="false"] { 
  position: static; 
  background-color: transparent; 
  opacity: 1; 
  z-index: auto; 
  width: auto;
  top: auto;
  border-bottom: none;
  box-shadow: none;
  padding-top: 0;
}

#presentation-view {
  padding: 9px 15px;
}

.no-present,
.present {
  position: absolute;
  float: right;
  top: -105px;
  right: -94px;
  z-index: 1003;
  font-size: 1.3em;
  width: 45px;
  color: @font-color-for-darkBG;
  background-color: @brand-secondary;
  padding: 4px 2px 7px;
  text-align: center;
  border-radius: 0 0 6px 6px;
  -webkit-transition: all .5s ease;
  -moz-transition: all .5s ease;
  -o-transition: all .5s ease;
  transition: all .5s ease;
  /* &:before {
    font-family: FontAwesome;
    content: '\f139';
  }

  &:after {
    font-family: @font-family-sans-serif;
    margin-left: 5px;
  } */
  &:hover, &:active, &:focus {
    background-color: lighten(@brand-secondary, 15%) !important;
    color: @font-color-for-darkBG;
    text-decoration: none;
  }
}

.present {
  top: 0;
  background-color: lighten(@brand-primary, 12%) !important;
  -webkit-transition: all .5s ease-in;
  -moz-transition: all .5s ease-in;
  -o-transition: all .5s ease-in;
  transition: all .5s ease-in;

  &.init-present {
    .notransition();
  }

  &:before {
    font-family: FontAwesome;
    content: '\f13a';
  }

  &:after {
    font-family: @font-family-sans-serif;
    margin-left: 5px;
  }

  &:hover, &:active, &:focus {
    color: @font-color-for-darkBG;
    text-decoration: none;
  }
}

#mgp-toolbar {
  opacity: 1;
  -o-transition: opacity .8s ease-in-out;
  -moz-transition: opacity .8s ease-in-out;
  -webkit-transition: opacity .8s ease-in-out;
  transition: opacity .8s ease-in-out;
  &.no-toolbar {
    opacity: 0;
    max-height: 0;
    overflow: hidden;
    -o-transition: all .5s ease-out;
    -moz-transition: all .5s ease-out;
    -webkit-transition: all .5s ease-out;
    transition: all .5s ease-out;
    // display: none;
  }
}



// Breadcrumbs directly below Main Nav
//------------------------------------
#breadcrumbs {
  background-color: @breadcrumb-bg;
  font-size: 0.95em;
  // offset top margin so breadcrumb bar is directly under navbar. Setting a negative top margin retains proper spacing under navbar for instances where the breadcrumb bar is not used
  /*margin-top: -53px;*/ // was -45px
  margin-bottom: 0; // 30px  No more whitespace per the CEO and CTO
  .breadcrumb {
    padding: 10px 0 0;
    /*background-color: transparent;*/
    margin-bottom: 10px;

    li.pull-right:last-child {
      //font-family: @breadcumb-client-font;
      font-size: 1.15em;
    }

    li.pull-right:last-child:before {
      content: '\e60c';
      font-family: font-mgp;
      margin-right: 1px;
      float: left;
      color: inherit;
    }
    // Mike please double check this stuff below - merging this removed some of the styles
    // above and changed some of the look and feel of the breadcrumbs unless that's how
    // you or Tony discussed it?
    li.crumb-right {
      float: right;
      font-size: 1.05em;

      &:last-child:before {
        content: '\e60c';
        font-family: font-mgp;
        margin-right: 1px;
        color: inherit;
      }
    }


    > li + li.crumb-right:before {
      content: none;
    }

    > li.crumb-right + li.crumb-right:after {
      content: '|';
      padding: 0 8px;
    }


    li {
      color: @breadcrumb-color;

      &.active {
        color: @breadcrumb-active-color;
      }
    }

    a {
      color: @breadcrumb-link-color;
    }
  }
}


// Mega Menu Styles
// http://jsfiddle.net/apougher/ydcMQ/
//------------------------------------
.menu-large {
  position: static !important;
}

.megamenu {
  padding: 10px 5px;
  /*width: 100%;*/
  max-width: @screen-xs-max;
}

.megamenu > li > .super-solve-button {
  .super-solve-button;
}

.megamenu > li > ul {
  padding: 0;
  margin: 0 0 10px 0;
}

.megamenu > li > ul > li {
  list-style: none;
}

.megamenu > li > ul > li > a {
  display: inline-block;
  padding: 3px;
  clear: both;
  font-weight: normal;
  line-height: 1.428571429;
  white-space: normal;
}

.megamenu > li ul > li > a:hover,
.megamenu > li ul > li > a:focus {
  background-color: @gray-lighter;
}

.megamenu .dropdown-header,
.megamenu .dropdown-header:hover {
  font-size: 1.1em;
  white-space: normal;
  padding: 3px;
}

.navbar-right .dropdown-menu {
  max-height: 500px;
  overflow-y: auto;
  right: auto;
}

/*Center Explore button under meter*/
.meter-results-section {
  display: inline-block;
  text-align: center;

  .details-menu {
    position: relative !important;
    width: auto;

    .dropdown-menu {
      top: auto;
      min-width: 260px;

      &.megamenu {
        max-height: 250px;
        overflow-y: auto;
      }
    }
  }

  .btn-cont-alt {
    margin-top: 0;

    .btn {
      margin-left: 10px;
      margin-right: 0;
    }

    .dropdown-menu {
      top: 22px;
    }
  }
}

// Results Menu
#results-details-nav {
  .dropdown-menu {
    max-height: 340px;
    left: 14px;
  }
}


// MGP Button & Button Container Styles
// -----------------------------------
// Use "btn-cont" class as a button container for right aligned buttons (i.e.- "Back, Next, Done, etc.) so positioning is consistent globally
.btn-cont {
  .clearfix(); /*clear: both;*/
  float: right;
  margin: 20px 0 30px;

  &.narrow {
    margin: 0;
  }

  .btn {
    margin-left: 30px;
    margin-bottom: 10px;
  }

  .dropdown-accordion .dropdown-menu {
    margin-top: 35px;
  }

  .explain-icon {
    margin-left: -20px;
  }
}


// Remove the outline onClick of button links
.btn-link {
  &:active,
  &:focus,
  &:hover,
  &:active:focus {
    outline: none;
    color: @link-hover-color;
  }
}


// Alternate LEFT aligned button container - for uses besides main form/application control (see .btn-cont above)
.btn-cont-alt {
  .clearfix(); /*clear: both;*/
  margin: 20px 0 0;
  display: block;

  .btn {
    margin-right: 30px;
    margin-bottom: 15px;
    float: left;
  }
  // Fix for Investment Assets "OR" text between buttons
  .non-btn-content {
    margin-right: 30px;
    margin-top: 5px;
    float: left;
  }

  .dropdown-accordion .dropdown-menu {
    margin-top: 35px;
  }

  .explain-icon {
    margin-left: -30px;
    float: left;
  }
}

.btn {
  -webkit-transition: background-color 1s ease-in-out;
  -moz-transition: background-color 1s ease-in-out;
  -o-transition: background-color 1s ease-in-out;
  transition: background-color 1s ease-in-out;
}

//For "click Next to skip" text on Loss Tolerance, Bear Market Reaction, etc.
.btn-forward-text {
  /*color: @btn-positive-action-bg;*/
  font-weight: bold;
}

.btn-primary {
  .button-variant(@btn-primary-color; @btn-primary-bg; @btn-primary-border);
}

.btn-secondary, .super-solve-button {
  .button-variant(@btn-secondary-color; @btn-secondary-bg; @btn-secondary-border);
}

.btn-tertiary {
  .button-variant(@btn-tertiary-color; @btn-tertiary-bg; @btn-tertiary-border);
}

.btn-forward, .btn-done {
  .button-variant(@btn-positive-action-color; @btn-positive-action-bg; @btn-positive-action-border);
}

.btn-back, .btn-cancel, .btn-close {
  .button-variant(@btn-negative-action-color; @btn-negative-action-bg; @btn-negative-action-border);
}

.btn-light {
  .button-variant(@btn-light-color; @btn-light-bg; @btn-light-border);
}

/*.btn-yodlee {
  .button-variant(@btn-yodlee-color; @btn-yodlee-bg; @btn-yodlee-border);
}*/

// Bootbox Plugin for Modal Style Overrides
.bootbox {
  .btn-primary {
    .button-variant(@btn-positive-action-color; @btn-positive-action-bg; @btn-positive-action-border);
  }

  // Reverse order of buttons on confirm modal - give button you want to be on the right (usually Cancel/No)- className: 'btn-default reverse-order' 
  .reverse-order {
    float: right;
    margin-left: 10px;
  }

  .yes-no-toggle .btn-primary {
    &:active,
    &:focus,
    &:hover {
      .button-variant(@btn-primary-color; @btn-primary-bg; @btn-primary-border);
    }
  }
}

.super-solve-button:after {
  font-family: font-mgp;
  content: "\e900";
  margin: 0 .3em 0 .3em;
  /* display: inline-block;
  font-size: 1.2em;
  margin: -5px 0;  // May need to remove this hacky approach - NT */
}


// Main Logo Styles
// -----------------------------------
.mainlogo {
  float: left;
  max-width: 570px; //set max-width so logo stays in correct proportions
  height: 60px;
  position: relative;

  img {
    overflow: auto;
    margin: auto;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    max-width: 570px;
    max-height: 60px;
  }
}


// Form Styles (general)
// -----------------------------------
// Remove/hide webkit input spinners/arrows on input type="number"
input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0;
}
// Required form fields places an asterisk on the form element
/*.required:after {
  color: @gray-light;
  content: " *";
  font-size: 1em;
}*/

.input-group-addon {
  min-width: 40px;
  background-color: @gray-lighter;
}

.input-group:not(td, th) {
  margin-bottom: 15px;
}

.form-horizontal {

  .input-group {
    margin-bottom: 0;
  }

  .form-group {
    clear: both;
  }

  .control-label label {
    display: inline;
  }
}

.form-inline {
  .form-group .input-group {
    margin-bottom: 0;
  }
}

.row-eq-height {
  height: 40px;
  margin-bottom: 15px;
}

// Hide Co-client labels unless <= Small Viewport (see media query below)
.hideCoClientLabels:nth-child(2) {

  .spacer-col,
  label:not(.radio-inline) {
    display: none;
  }
  .spacer-col,
  legend {
     position: absolute; 
     overflow: hidden; 
     clip: rect(0 0 0 0); 
  }

}

// Used for form fields that display a total (highlighted in green by default)
.input-total {
  font-weight: bold;
  //institutions not liking this so instead bolded totals
  //.form-control-validation(@state-success-text; @state-success-text; @state-success-bg);
}


// Form Validation Styles
//------------------------------------
.has-error.input-total {
  .form-control-validation(@alert-danger-text; @alert-danger-text; @alert-danger-bg);
}

.has-error .input-total .input-group-addon {
  color: @alert-danger-text;
  background-color: @alert-danger-bg;
  border-color: @alert-danger-text;
}

.has-error .input-total .form-control {
  border-color: @alert-danger-text;
}

.table > tfoot > tr.totals > td.has-error {
  color: @alert-danger-text;
  background-color: @alert-danger-bg;
}

.has-error .required:after,
.has-error label,
.has-error .input-total .help-error-info {
  color: @alert-danger-text;
}

.has-error input[type=radio],
.has-error .yes-no-toggle {
  outline: 1px solid @alert-danger-text;
  margin-right: .5em; //leave space so error text isn't touching
}

.has-error .input-valid {
  outline: none;
  border-color: @input-border;
}

.has-error .input-valid:focus {
  border-color: @input-border-focus;
  outline: none;
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px @input-border-focus; //rgba(102, 175, 233, 0.6);
}

.has-error .input-valid .input-group-addon {
  color: @gray;
  border-color: @input-border;
  background-color: @gray-lighter;
}

.has-warning .input-group-addon {
  color: @brand-warning;
  border-color: @brand-warning;
  background-color: @alert-warning-bg;
}

.has-warning .form-control,
.has-warning .form-control:focus {
  border-color: @brand-warning;
}

/*Hide help-error-info when valid prevent jumping and spacing issue on submit*/
.help-error-info.valid {
  display: none;
}

/*We use .help-block for text like "today's dollars, after-tax" and .help-error-info for validation error msgs */
.help-error-info {
  .help-block;
  color: @alert-danger-text;
}

.has-error .help-block {
  color: @text-color; /*lighten(@text-color, 25%); <- doesn't pass 508 color contrast test */
}


// Dropdown Menu Styles
//------------------------------------
// Multi-select dropdown styling
.btn-multiselect { // same as .form-control
  line-height: @line-height-base;
  color: @input-color;
  background-color: @input-bg;
  background-image: none; // Reset unusual Firefox-on-Android default style; see https://github.com/necolas/normalize.css/issues/214
  border: 1px solid @input-border;
  border-radius: 0 3px 3px 0;

  &:hover,
  &:focus {
    color: inherit;
  }

  .btn-group .caret {
    position: absolute;
    right: 10px;
    top: 15px;
  }
}

.multiselect-container {

  &.dropdown-menu > .active > a {
    background-color: @input-bg;
    color: @input-color;
  }
}

ul.multiselect-container.dropdown-menu {
  overflow: hidden;
  overflow-y: auto;
  white-space: nowrap;
  -ms-text-overflow: ellipsis;
