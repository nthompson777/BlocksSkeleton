// Media Queries
// ----------------------------------------

// Presentation Mode for Lg Viewports Breakpoint
// ----------------------------------------
@media (min-width:1201px) and (max-width:1330px) {
  .no-present, .present {
    position: fixed;
    top: 120px;
    right: 0;
    border-radius: 6px 0 0 6px;
  }
}

// >= Medium or Larger Viewport
// ----------------------------------------
@media (min-width:@screen-md-min) {
  .text-right-md {
    text-align: right;
  }
}


// Medium ONLY!!! Viewport
// ----------------------------------------
@media (min-width:@screen-md-min) and (max-width:@screen-md-max) {
  .slick-next {
    right: -30px;
  }

  .no-present, .present {
    position: absolute;
    top: -105px;
    right: -94px;
    border-radius: 0 0 6px 6px;
  }

  .present {
    top: -6px;
  }

  ul.progressbar > li:before {
    width: 20% !important; // 45%
    left: -8% !important; // -20%
  }
  
  #available-goals {
    .goal-cell:nth-of-type(1),
    .goal-cell:nth-of-type(9) {
      margin-left: 0;
    }

    .goal-cell {
      width: 110px;
    }
  }

  .goal-summary-table .goal-card {
    height: 70px;
    width: 70px;
    max-height: 70px;
    max-width: 70px;
  }
  
  #goals-masterpiece .hover-highlight:hover:before {
    top: 45px;
  }

  #available-goals .available-draggable.ui-draggable {
    position: relative;
    height: 110px;
  }
}


// Presentation Mode for Md Viewports Breakpoint
// ----------------------------------------
@media (max-width:1125px) {
  .no-present, .present {
    position: fixed;
    top: 105px;
    right: 0;
    border-radius: 6px 0 0 6px;
  }
}


// <= Medium or Smaller Viewport
// ----------------------------------------
@media (max-width:@screen-md-max) {
  .yodlee-dialog .modal-dialog,
  .mx-dialog .modal-dialog {
    width: 100%;
  }
  //Only hide Admin menu if viewport md or less
  #adminMenu.offcanvas-left {
    &.active-menu-item {
      left: 0;
      margin-left: 0;
    }
    .offcanvas-default-menu {
      display: none;
    }
  }
}


// >= Small or Larger Viewport
// ----------------------------------------
@media (min-width:@screen-sm-min) {
  .text-right-sm {
    text-align: right;
  }

  .text-center-sm {
    text-align: center;
  }

  .navbar-right .dropdown-menu {
    right: auto;
  }
}


// Small ONLY!!! Viewport
// ----------------------------------------
@media (min-width:@screen-sm-min) and (max-width:@screen-sm-max) {
  .chart-wrapper {
    padding-bottom: 100%;
  }

  /* Use ONLY on sm Viewport when labels wrap to 2 lines and spacing is thrown off from hiding co-client labels on that col (see example on G4 Retirement Goal Retirement and Planning Age  */
  .form-group.multi-line {
    margin-top: 18px;
  }
}


// <= Small Viewport
// ----------------------------------------
@media (max-width: @screen-sm-max) {

  .mainlogo {
    margin-left: 15px;
  }

  // Presentation Mode for Tablets
  .present {
    top: 5px;
  }

  ul.progressbar > li:before {
    display: none;
  }

  #breadcrumbs {
    margin-bottom: 0; // No more whitespace per the CEO and CTO

    .breadcrumb {
      padding: 10px 0 0;
      background: transparent;
    }
  }

  ul.progressbar>li a.dropdown-toggle,
  ul.progressbar>li>a {
    font-size: 1.1em;
  } 

  //Handles cases where there is a navbar, but no breadcrumbs (guest using MyMg frame)
  //Couldn't set margin or padding bottom of .navbar-default b/c it is absolutely positioned
  //so had to add the spacer element #no-breadcrumbs for these cases
  #no-breadcrumbs {
    padding-bottom:60px; //Do not change this without talking to MLK!!!
  }

  .container>.navbar-collapse {
    background: #fff;
  }

  .navbar-collapse.collapse.in {
    box-shadow: -5px 25px 30px -26px rgba(0,0,0,.35);
  }

  .navbar {
    border: none;
  }

  .navbar-nav {
    float: left;
    font-size: 1.2em;
    /*max-height: 60px;
    margin-top: 60px;   
    margin-bottom: 0;*/
  }

  .navbar-right .dropdown-menu {
    max-height: none;
  }

  // Form Styles
  .form-horizontal {

    .radio-inline,
    .checkbox-inline {
      margin-bottom: 15px; // compensate for clearing the float on mobile
      padding-top: 7px;
    }

    .radio {
      min-height: 22px;
    }
  }

  // Fix for stacked form fields that are in the same form-group or row (please keep in this cascading order!)
  .form-group,
  .row {

    .form-control { // Do not add :not here - which kills the fix!  See below...
      margin-bottom: 10px;
    }

    .input-group .form-control {
      margin-bottom: 0;
    }

    .input-group:nth-of-type(1), // Do not add :not to nth-of-type here - which kills the fix!  See below...
    .clearfix.visible-sm-block {
      margin-bottom: 10px;
    }
  }

  th, td {

    .form-control {
      margin-bottom: 0;
    }

    .form-group,
    .row {

      .input-group:nth-of-type(1),
      .clearfix.visible-sm-block {
        margin-bottom: 0;
      }
    }
  }

  // Typography - Scale down slightly to free up space for smaller viewports
  h1 {
    font-size: 2.0em;
  }

  h2 {
    font-size: 1.65em;
  }

  h3 {
    font-size: 1.45em;
  }

  .dropdown-accordion > .btn-block + .dropdown-menu {
    margin-top: 0;
  }

  // Off-Canvas Menus
  .offcanvas-default-menu {
    position: absolute;
    top: 0;
    width: 35%;
  }

  .offcanvas-left {

    .offcanvas-default-menu {
      left: -39%;
    }

    &.active-menu-item {
      left: 40%;
    }
  }

  // "Last Updated" date display style
  .last-updated {
    float: left;
    margin-bottom: 20px;
  }

  .chart-inner {
    position: absolute;
  }

  // Mega Menu Styles for Main Nav Dropdown
  .megamenu {
    padding: 10px 0;

    > li {
      margin-bottom: 10px;
    }

    .dropdown-header {
      padding: 3px !important;
    }
  }

  .table-responsive .dataTables_wrapper {
    overflow: visible;
  }

  .slider-container {
    position: relative;
    margin: 15px 5px 30px 0; // keep the right margin due to needing the offset for <= sm viewports when slider value pushes off the viewport -when slider is pushed to the max value
  }

  // Mobile Menu Animation from "Hamburger" menu to "X"
  .navbar-toggle {
    border: none;
    background: transparent !important;
    margin-right: 5px; // aligns hamburger menu with the grid (only use this margin spacing if no border around hamburger menu)
    &:hover {
      background: transparent !important;
    }

    .icon-bar {
      width: 22px;
      -moz-transition: all 0.2s;
      -o-transition: all 0.2s;
      -webkit-transition: all 0.2s;
      transition: all 0.2s;
    }

    .top-bar {
      -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
      -o-transform: rotate(45deg);
      -webkit-transform: rotate(45deg);
      transform: rotate(45deg);
      -moz-transform-origin: 10% 10%;
      -ms-transform-origin: 10% 10%;
      -o-transform-origin: 10% 10%;
      -webkit-transform-origin: 10% 10%;
      transform-origin: 10% 10%;
    }

    .middle-bar {
      opacity: 0;
    }

    .bottom-bar {
      -moz-transform: rotate(-45deg);
      -ms-transform: rotate(-45deg);
      -o-transform: rotate(-45deg);
      -webkit-transform: rotate(-45deg);
      transform: rotate(-45deg);
      -moz-transform-origin: 10% 90%;
      -ms-transform-origin: 10% 90%;
      -o-transform-origin: 10% 90%;
      -webkit-transform-origin: 10% 90%;
      transform-origin: 10% 90%;
    }

    &.collapsed {

      .top-bar {
        -moz-transform: rotate(0);
        -ms-transform: rotate(0);
        -o-transform: rotate(0);
        -webkit-transform: rotate(0);
        transform: rotate(0);
      }

      .middle-bar {
        opacity: 1;
      }

      .bottom-bar {
        -moz-transform: rotate(0);
        -ms-transform: rotate(0);
        -o-transform: rotate(0);
        -webkit-transform: rotate(0);
        transform: rotate(0);
      }
    }
  }

  .goal-carousel {
    height: 80px;
    margin-left: 30px;
    margin-top: 0;

    .slick-prev {
      left: 0;
    }

    .slick-next {
      right: 0;
    }
  }

  // Goal Styles
  .goal-summary-table {

    .goal-card {
      height: 80px;
      width: 80px;
      max-height: 80px;
      max-width: 80px;
    }
    
    .highlight {
      width: inherit;
    }

    .hover-highlight {
      border-right: none;
    }

    .droppable, .selected-droppable {
      padding: 0 0 75px;
      margin-bottom: -60px;
    }

    /*.delete-goal {
      margin-right: 0;
    }*/

    .mobile-handle {
      cursor: move;
      opacity: .45;
      height: 40px;
    }
  }

  // Goals Compact View Only
  .compact-view {
    
    .mobile-handle {
      display: none;
    }
  }

  #available-goals {
    .goal-cell:nth-of-type(1),
    .goal-cell:nth-of-type(9) {
      margin-left: 0;
    }

    .goal-cell {
      width: 100px;
    }
  }

  #available-goals .goal-cell {
    min-height: 120px;
  }

  // Shows the Co-client labels when stacked on Xsmall Viewports
  .soc-security .hideCoClientLabels:nth-child(2),
  .side-by-side-clients .hideCoClientLabels:nth-child(2) {

    .spacer-col,
    label {
      display: inline-block;
    }
    .spacer-col,
    legend {
      position: relative; 
      display: inline-block;
      overflow: visible; 
    }
  }

  .side-by-side-clients {
   .control-label {
      text-align: left;
    }
  }

  .row-eq-height {
    height: inherit;
    margin-bottom: 15px;
  }

  // Goals Styles
  #available-goals {
    
    .goal-card {
      height: 45px;
    }

    .goal-cell {
      min-height: 80px;
      width: 100px;
    }
  }

  .goal-summary-table .hover-highlight {
    width: inherit;
  }

  .available-draggable, 
  .draggable {
    font-size: .75em;
  }

  .goal-container h4 {
    font-size: 1.2em;
  }

  .view-only {

    #goals-masterpiece&,
    #goals-summary& {

      .mobile-handle {
        display: none;
      }
    }
  }
}


// Xsmall ONLY!!! Viewport
// ----------------------------------------
@media (max-width: @screen-xs-max) {
  /*body {
    margin-bottom: 250px; //for large disclosures
  }*/
  .mainlogo img {
    max-width: 210px; //175px;
  }

  section {
    padding: 0 0 15px;
  }

  .navbar-default.navbar-fixed-top {
