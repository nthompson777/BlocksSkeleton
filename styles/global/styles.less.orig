    margin-top: 3px;
    margin-bottom:0;

    .icon-bar {
      background-color: @navbar-default-toggle-icon-bar-bg;
    }
  }
  // Hide Presentation View on Xsmall Viewports and Override the Styles in case it is enabled on Small Viewport size and orientation is changed to XS
  #presentation-view {
    display: none;
  }

  .no-toolbar {
    opacity: 1 !important;

    &.hidden {
      display: block !important;
    }
  }

  /*body.no-TopPadding {
    padding-top: 70px;
  }*/

  /* .navbar-nav {
    margin-top: 0;
   
    &.navbar-right {
      margin-top: 0;
      width: 100%;
    }
  } */
 #breadcrumbs {
    .breadcrumb.hidden-xs {
      display: inline-block !important;
      margin-top: 0; // 60px
      text-align: center;

      li {
        display: none;
      }

      li.crumb-right:last-child {
        display: inline-block;
        text-align: center;
        float: none !important;
        padding: 0;
        font-size: 1em;
        clear: both;

        &:after {
          content: none;
        }
      }
    }
  }

  .visible-xs#breadcrumbs {
    // background-color: transparent;
    // margin-bottom: 0;
    margin-top: 0;
    //border-bottom: solid 1px #eee;
    font-size: 0.85em;
    background: @breadcrumb-bg; /* Old browsers */
    background: linear-gradient(to right, @breadcrumb-bg 57%, @font-color-for-darkBG 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='@breadcrumb-bg', endColorstr='@font-color-for-darkBG',GradientType=1 ); /* IE6-9 */

    .breadcrumb {
      padding: 10px 0 10px 15px;
      margin-bottom: 0;
      padding-left: 15px;
      background: @breadcrumb-bg !important;
      //width: 104%;
    }
  }

  ul.progressbar {
    margin-top: 0;
    margin-bottom: 15px;

    > li > a {
      font-size: 0.85em;
    }

    & > li:before {
      display: none;
    }
  }

  .hideClientLabels {
    display: none;
  }

  // Shows the Co-client labels when stacked on Xsmall Viewports
  .hideCoClientLabels:nth-child(2) {

    .spacer-col,
    label {
      display: inline-block;
    }
    .spacer-col,
    legend {
      position: relative; 
      display: inline-block;
      overflow: visible;  
    }
  }

  // Setting font-size to >= 16px on form input(s) disables iOS auto-zoom when focus is on form input field (since we allow pinch and zoom on our application)
  input, select {
    &.form-control {
      font-size: 1.15em;  // == 16px
    }
  }

  .form-control {
    height: 35px; // bump up the height 1px to accomodate the font size increase so the bottoms of y's and g's aren't cut off
  }


  .chart-wrapper {
    padding-bottom: 150%;
  }

  .scrollToTop {
    bottom: 10%; // accomodate for copyright visibility
  }

  // Mega Menu
  // http://jsfiddle.net/apougher/ydcMQ/
  .megamenu {
    margin-left: 0;
    margin-right: 0;

    > li:last-child {
      margin-bottom: 0;
    }

    .dropdown-header {
      padding: 3px /*15px*/ !important;
    }
  }

  // ion Range Slider
  .slider-container {
    position: relative;
    margin: 10px 0; // keep the right margin due to needing the offset for <= sm viewports when slider value pushes off the viewport -when slider is pushed to the max value
  }

  // Auto Scroll Elements that need to Stay Full Width on xs Viewport Size
  .overflow-cont {
    overflow-x: auto;
    overflow-y: hidden;
    white-space: nowrap;
    padding-bottom: 15px; // give some padding for the scroll bar
    > .row [class*="col-lg"],
    > .row [class*="col-md"],
    > .row [class*="col-sm"] {
      float: none;
      display: inline-block;
      white-space: normal;
      vertical-align: top;
    }

    .overflow-block {

      &.goals {
        min-width: 400px;
      }
    }

    h5 {
      margin-top: 20px;
      margin-bottom: 5px;
    }
  }

  .indent-1:not(td),
  .indent-2:not(td) {
    padding-left: 0;
  }

  .yodlee-dialog .modal-body,
  .mx-dialog .modal-body {
    height: 720px;
  }

  //Override Bootstrap from no-wrapping tables cells on xs viewport
  .table-responsive {
    > .table {
      > thead,
      > tbody,
      > tfoot {
        > tr {
          > th,
          > td {
            white-space: normal;
          }
        }
      }
    }
  }

  // Goal Styles
  .goal-summary-table {

    .goal-card {
      height: 50px;
      width: 50px;
      max-height: 50px;
      max-width: 50px;
    }

    #available-goals .goal-cell {
      width: 90px;
    }

    .goal-details {
      margin-left: 0;
      padding-top: 5px;
    }

    .delete-goal {
